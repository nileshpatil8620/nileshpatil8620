#include<stdio.h>
#include<conio.h>
void main()
{
    int num;
    printf("\n\n For Checking given number +ve or -ve");
    printf("\n\n Enter any Number = ");
    scanf("%d",&num);
    
    if ( num > 0 )
       printf("\n\n %d is Positive", num );
    else
       printf("\n\n %d is Negative", num );
       
}




